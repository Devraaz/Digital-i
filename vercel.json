{
    "version":2,
    "builds":[
        {
            "src":"poll/wsgi.py",
            "use":  "@vercel/python",
            "config": { "maxLambdaSize": "limb", "runtime": "python3.9" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src":"/(.*)",
            "dest": "poll/wsgi.py"
        }
    ]
} 